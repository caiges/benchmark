{
    "name": "noahjs/benchmark",
    "type": "application",
    "description": "This is a PHP framework Benchmark test",
    "keywords": ["benchmark"],
    "license": "open",
    "authors": [
        {
            "name": "Noah Spirakus",
            "email": "noah@noahjs.com"
        }
    ],
	"require": {
		"laravel/framework": "4.1.*",
        "symfony/yaml": "*"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}